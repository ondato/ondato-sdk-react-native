{
    "captureResolution": "720",
    "pictureResolution": "1080",
    "cutout": {
        "ratioFromSize": {
            "width": 10,
            "height": 7
        },
        "alignment": "center",
        "offset": {
            "x": 0,
            "y": 0
        },
        "outerColor": "000000",
        "outerAlpha": 0.7,
        "style": "rect",
        "strokeColor": "FFFFFF",
        "feedbackStrokeColor": "FFFFFF",
        "strokeWidth": 2,
        "cornerRadius": 4,

    },
    "flash": {
        "mode": "manual",
        "alignment": "top_right",
        "imageOn": "ic_flash_on",
        "imageOff": "ic_flash_off",
        "imageAuto": "ic_flash_auto",
        "offset": {
            "x": 10,
            "y": -10
        }
    },
    "beepOnResult": false,
    "vibrateOnResult": false,
    "blinkAnimationOnResult": true,
    "cancelOnResult": true,
}
